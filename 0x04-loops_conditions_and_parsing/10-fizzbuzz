#!/usr/bin/env bash
# This script displays 0 to 100 but
# Displays FizzBuzz when the number is a multiple of 3 and 5
# Displays Fizz when the number is multiple of 3
# Displays Buzz when the number is a multiple of 5
for ((i = 0; i <= 100; i++))
do
    if ((i == 0 || i == 0))
    then
        echo "$i"
    elif ((i % 3 == 0 && i % 5 == 0))
    then
        echo "FizzBuzz"
    elif ((i % 3 == 0))
    then
        echo "Fizz"
    elif ((i % 5 == 0))
    then
        echo "Buzz"
    else
        echo "$i"
    fi
done
